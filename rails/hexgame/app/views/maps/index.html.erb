<h1>Listing maps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Game</th>
    <th>Size hx</th>
    <th>Size hy</th>
    <th>Origin hx</th>
    <th>Origin hy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maps.each do |map| %>
  <tr>
    <td><%= map.name %></td>
    <td><%= map.game %></td>
    <td><%= map.size_hx %></td>
    <td><%= map.size_hy %></td>
    <td><%= map.origin_hx %></td>
    <td><%= map.origin_hy %></td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map', new_map_path %>
